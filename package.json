{
  "name": "sfdx-ext",
  "description": "sfdx extensions",
  "version": "0.0.29",
  "author": "Brian Edward Saunders",
  "bugs": "https://github.com/brianedwardsaunders/sfdx-ext/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/errors": "^1",
    "@salesforce/command": "^1.4.1",
    "@salesforce/core": "^1.3.2",
    "@types/fs-extra": "^7.0.0",
    "@types/yauzl": "^2.9.1",
    "tslib": "^1",
    "xml-js": "^1.6.11",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/plugin-help": "^2.2.1",
    "@oclif/test": "^1.2.5",
    "@salesforce/dev-config": "1.4.1",
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5",
    "@types/node": "^10.17.3",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "rimraf": "^2.7.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "~3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/brianedwardsaunders/sfdx-ext",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to augment sfdx-cli."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "brianedwardsaunders/sfdx-ext",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}

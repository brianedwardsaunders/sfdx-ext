{
  "name": "sfdx-ext",
  "description": "sfdx extensions",
  "version": "0.0.54",
  "author": "Brian Edward Saunders",
  "bugs": "https://github.com/brianedwardsaunders/sfdx-ext/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.3.4",
    "@salesforce/command": "^2",
    "@salesforce/core": "^2.16.3",
    "@sinonjs/fake-timers": "^6.0.1",
    "@types/sinon": "9.0.0",
    "@types/sinonjs__fake-timers": "^6.0.4",
    "@types/fs-extra": "^7.0.0",
    "@types/yauzl": "^2.9.1",
    "tslib": "^1",
    "xml-js": "^1.6.11",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1.2.8",
    "@salesforce/dev-config": "1.4.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10.17.51",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^14",
    "rimraf": "^3.0.2",
    "ts-node": "^8",
    "tslint": "^5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/brianedwardsaunders/sfdx-ext",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to augment sfdx-cli."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "brianedwardsaunders/sfdx-ext",
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}

{
  "name": "sfdx-ext",
  "description": "sfdx extensions to retrieve and compare salesforce metadata from multiple orgs",
  "version": "0.1.1",
  "author": "brian.edward.saunders@gmail.com",
  "bugs": "https://github.com/brian.edward.saunders/sfdx-ext/issues",
  "dependencies": {
    "@oclif/core": "1.20.2",
    "@salesforce/command": "5.2.18",
    "@salesforce/core": "3.31.17",
    "@types/fs-extra": "9.0.13",
    "tslib": "2.4.1",
    "xml-js": "1.6.11",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@salesforce/ts-sinon": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^8",
    "@types/yauzl": "^2.10.0",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "chai": "^4",
    "eslint": "^7",
    "eslint-config-prettier": "^8",
    "eslint-plugin-header": "^3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsdoc": "^35",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-typescript": "^0",
    "mocha": "^9",
    "nyc": "^15",
    "oclif": "^3",
    "prettier": "^2",
    "rimraf": "^3.0.2",
    "sinon": "10.0.0",
    "ts-node": "^10",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/brian.edward.saunders/sfdx-ext",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    }
  },
  "repository": "brian.edward.saunders/sfdx-ext",
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint src/**/*.ts test/**/*.ts",
    "prepack": "rimraf lib && tsc -b && oclif manifest && oclif readme",
    "test": "nyc --timeout 600000 --extension .ts --require ts-node/register mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  }
}
